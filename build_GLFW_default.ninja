# project build file
# generated with premake ninja

ninja_required_version = 1.6

# core rules for default
rule cc
  command = gcc -m64 -w -D_GLFW_WIN32 -D_CRT_SECURE_NO_WARNINGS -MMD -MF $out.d -c -o $out $in
  description = cc $out
  depfile = $out.d
  deps = gcc

rule cxx
  command = g++ -m64 -w -MMD -MP -m64 -w -D_GLFW_WIN32 -D_CRT_SECURE_NO_WARNINGS -MMD -MF $out.d -c -o $out $in
  description = cxx $out
  depfile = $out.d
  deps = gcc

rule ar
  command = ar rcs $out $in
  description = ar $out

rule link
  command = gcc -o $out $in  -lgdi32  -L/usr/lib64 -m64 -s
  description = link $out

rule custom_command
  command = $CUSTOM_COMMAND
  description = $CUSTOM_DESCRIPTION

# build files
build vendor/GLFW/bin-int/default/GLFW/context.o: cc vendor/GLFW/src/context.c
build vendor/GLFW/bin-int/default/GLFW/egl_context.o: cc vendor/GLFW/src/egl_context.c
build vendor/GLFW/bin-int/default/GLFW/init.o: cc vendor/GLFW/src/init.c
build vendor/GLFW/bin-int/default/GLFW/input.o: cc vendor/GLFW/src/input.c
build vendor/GLFW/bin-int/default/GLFW/monitor.o: cc vendor/GLFW/src/monitor.c
build vendor/GLFW/bin-int/default/GLFW/null_init.o: cc vendor/GLFW/src/null_init.c
build vendor/GLFW/bin-int/default/GLFW/null_joystick.o: cc vendor/GLFW/src/null_joystick.c
build vendor/GLFW/bin-int/default/GLFW/null_monitor.o: cc vendor/GLFW/src/null_monitor.c
build vendor/GLFW/bin-int/default/GLFW/null_window.o: cc vendor/GLFW/src/null_window.c
build vendor/GLFW/bin-int/default/GLFW/osmesa_context.o: cc vendor/GLFW/src/osmesa_context.c
build vendor/GLFW/bin-int/default/GLFW/platform.o: cc vendor/GLFW/src/platform.c
build vendor/GLFW/bin-int/default/GLFW/vulkan.o: cc vendor/GLFW/src/vulkan.c
build vendor/GLFW/bin-int/default/GLFW/wgl_context.o: cc vendor/GLFW/src/wgl_context.c
build vendor/GLFW/bin-int/default/GLFW/win32_init.o: cc vendor/GLFW/src/win32_init.c
build vendor/GLFW/bin-int/default/GLFW/win32_joystick.o: cc vendor/GLFW/src/win32_joystick.c
