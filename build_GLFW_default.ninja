# link static lib
build vendor/GLFW/bin/default/GLFW/GLFW.lib: ar vendor/GLFW/bin-int/default/GLFW/context.o vendor/GLFW/bin-int/default/GLFW/egl_context.o vendor/GLFW/bin-int/default/GLFW/init.o vendor/GLFW/bin-int/default/GLFW/input.o vendor/GLFW/bin-int/default/GLFW/monitor.o vendor/GLFW/bin-int/default/GLFW/null_init.o vendor/GLFW/bin-int/default/GLFW/null_joystick.o vendor/GLFW/bin-int/default/GLFW/null_monitor.o vendor/GLFW/bin-int/default/GLFW/null_window.o vendor/GLFW/bin-int/default/GLFW/osmesa_context.o vendor/GLFW/bin-int/default/GLFW/platform.o vendor/GLFW/bin-int/default/GLFW/vulkan.o vendor/GLFW/bin-int/default/GLFW/wgl_context.o vendor/GLFW/bin-int/default/GLFW/win32_init.o vendor/GLFW/bin-int/default/GLFW/win32_joystick.o vendor/GLFW/bin-int/default/GLFW/win32_module.o vendor/GLFW/bin-int/default/GLFW/win32_monitor.o vendor/GLFW/bin-int/default/GLFW/win32_thread.o vendor/GLFW/bin-int/default/GLFW/win32_time.o vendor/GLFW/bin-int/default/GLFW/win32_window.o vendor/GLFW/bin-int/default/GLFW/window.o 

build GLFW_default: phony vendor/GLFW/bin/default/GLFW/GLFW.lib
